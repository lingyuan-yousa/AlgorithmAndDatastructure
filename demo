#include<bits/stdc++.h>
using namespace std;
class Solution {
public:
    int rob(vector<int>& nums) {
        if (nums.size() == 0) return 0;
        if (nums.size() == 1) return nums[0];
        if (nums.size() == 2) return max(nums[0], nums[1]);
        int ans = 0;
        vector<int> dp(nums.size() + 1, 0);
        dp[0] = nums[0];
        dp[1] = nums[1];
        ans = max(dp[0], dp[1]);
        for (int i = 2; i < nums.size(); ++i) {
            for (int j = i - 2; j >= 0; --j) {
                dp[i] = max(dp[j] + nums[i], dp[i]);
            }
            ans = max(ans, dp[i]);
        }
        return ans;
    }
};
int main() {
    system("pause");
    return 0;
}
